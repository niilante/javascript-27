{
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "array-bracket-spacing": 2,
    "arrow-spacing": 2,
    "brace-style": 2,
    "camelcase": [2, { properties: "never" }],
    "comma-dangle": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "curly": [2, "all"],
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "func-call-spacing": 2,
    "func-style": ["error", "declaration"],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "max-depth": [2, 5],
    "max-len": [2, { "code": 120 }],
    "max-params": [2, 6],
    "new-cap": 2,
    "no-alert": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": 2,
    "no-confusing-arrow": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": [2, { "allowEmptyCatch": false }],
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new-func": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal-escape": 2,
    "no-path-concat": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": [2, "always"],
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-useless-concat": 2,
    "no-useless-rename": 2,
    "no-var": 2,
    "no-void": 2,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "object-shorthand": [2, "always", {"avoidQuotes": true}],
    "one-var": [2, "never"],
    "one-var-declaration-per-line": [2, "always"],
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single", { "avoidEscape": true }],
    "semi": 2,
    "semi-spacing": 2,
    "space-before-blocks": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "strict": [2, "global"],
    "template-curly-spacing": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "inside"],
    "yoda": 2
  }
}
